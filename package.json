{
  "name": "chaoticbacktalk",
  "version": "3.2.0",
  "description": "Discord Bot for the Chaotic Discord",
  "author": "Danude Sandstorm",
  "scripts": {
    "start": "set NODE_ENV=development&& babel-node src/bot.js --presets env,stage-1",
    "build": "babel src -d build --copy-files",
    "forever": "forever -o out.log -e err.log start build/bot.js",
    "rebuild": "npm run stop && npm run build && npm run forever",
    "stop": "forever stop build/bot.js",
    "clean": "rm -rf build/",
    "gitauto": "forever start node_modules/git-auto-pull/git-auto-pull.js ../discordbot/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chaoticbackup/discordbot.git"
  },
  "license": "MIT",
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "bufferutil": "^4.0.0",
    "discord.js": "^11.4.0",
    "fs-extra": "^7.0.1",
    "jquery": "^3.3.1",
    "jsdom": "^13.0.0",
    "lokijs": "^1.5.5",
    "node-fetch": "^2.3.0",
    "path": "^0.12.7",
    "pdfjs-dist": "^2.0.943",
    "winston": "^3.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-1": "^6.24.1",
    "forever": "https://github.com/chaoticbackup/forever.git",
    "git-auto-pull": "^1.0.9"
  }
}
