{
  "env": {
    "node": true,
    "es6": true
  },
  "parser": "babel-eslint",
  "parserOptions": { 
    "ecmaVersion": 8,
    "sourceType": "module",
    "ecmaFeatures": {
      "modules": true,
      "experimentalObjectRestSpread": true
    }
  },
  "globals": {},
  "rules": {
    "eqeqeq": 0,
    "dot-notation": 0,
    "no-unused-vars": 1,
    "no-undef": 2
  }
}
